<tr>
   <td><%= job.title %></td>
   <td><%= job.description %></td>
   <td>
       <%= link_to "Edit", edit_job_path(job), remote: true, class: "btn btn-primary btn-xs" %>
   </td>
   <td><%= link_to job, remote: true, method: :delete, data: { confirm: 'Are you sure?' } do %>
            <button class="btn btn-danger btn-xs">Delete!</button>
        <% end %>
   </td>
</tr>



